set foldmethod=marker
set nocompatible              " be iMproved, required
let g:indentLine_char_list = '|'
set cindent
set complete=.,w,k
set hidden
set nocp
set keywordprg=dict
set nocompatible
set clipboard=unnamedplus
inoremap ;; <C-n>
set novisualbell
set background=dark
set pastetoggle=<F2>
set tabstop=2
set smarttab
set undofile
set undodir=~/.vim/undodir
set shiftwidth=2
set expandtab
set guioptions-=r
set guioptions-=R
set guioptions-=l
set guioptions-=L
set number relativenumber
set nu rnu
set incsearch
set autoindent
set ruler
set autowrite
set autoread
set linebreak
" set spell
set et
set title
filetype indent on
set noeb vb t_vb=
set history=500
set hlsearch
set lazyredraw
set magic
set backspace=indent,eol,start
set showmatch
set ignorecase
set smartcase
set mat=2
set lbr
set backspace= eol
set tw=600
set wrap
set ai 
set si 
set matchtime=2 
set matchpairs+=<:>
set so=7
set wildmenu
set wildmode=longest,list

syntax enable
